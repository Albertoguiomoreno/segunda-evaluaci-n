#include<stdio.h>

int main(){
	int numero[10];
	int mayor;
	int cont;
	//leemos 10 numeros
	for(cont=0;cont<10;cont++){
		printf("numero %d: ",cont);
		scanf("%d",&numero[cont]);
	}
	//los mostramos en pantalla
	printf("*************************");
	printf("** la lista **");
	printf("*************************");
	for(cont=0;cont<10;cont++){
		printf("\nNumero %d: %d",cont,numero[cont]);
	}	
	return 0;
}
